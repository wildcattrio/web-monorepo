{
  "hosting": [
    {
      "target": "wildcattrio-web-prod",
      "public": "apps/wildcattrio-site/public",
      "ignore": ["**/.*", "**/node_modules/**"],
      "headers": [
        {
          "source": "**",
          "headers": [
            { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }
          ]
        }
      ],
      "rewrites": [
        { "source": "/", "destination": "/index.html" }
      ]
    },
    {
      "target": "wildcattrio-www",
      "public": "apps/wildcattrio-site/empty",
      "ignore": ["**/.*", "**/node_modules/**"],
      "headers": [
        {
          "source": "**",
          "headers": [
            { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }
          ]
        }
      ],
      "redirects": [
        { "source": "/**", "destination": "https://wildcattrio.com", "type": 301 }
      ]
    },
    {
      "target": "elevatequotes-web-prod",
      "public": "apps/elevatequotes-site/public",
      "ignore": ["**/.*", "**/node_modules/**"],
      "headers": [
        {
          "source": "**",
          "headers": [
            { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }
          ]
        }
      ]
    },
    {
      "target": "calsmartai-web-prod",
      "public": "apps/calsmartai-site/public",
      "ignore": ["**/.*", "**/node_modules/**"],
      "headers": [
        {
          "source": "**",
          "headers": [
            { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }
          ]
        }
      ]
    }
  ]
}


